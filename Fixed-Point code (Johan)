from math import exp

def g(c):
  return 668.06/c * (1-exp(-0.146843*c))-40


def fixPoint(x0, es, Imax):
    xr = x0
    Iter = 0
    for i in range(10):
        xrold = xr
        xr = g(xrold)
        Iter = Iter + 1
        if xr != 0:
            ea = ((xr-xrold)/xr)*100
        if ea < es or Iter >= Imax:
            break
    return xr

fixPoint(5,1,3)
